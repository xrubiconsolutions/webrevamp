{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/product/index.jsx",
    "src/components/Products/KPI/Benefit.tsx",
    "src/components/Products/index.jsx",
    "src/components/Products/Flex.tsx",
    "src/components/Products/Earn/Earnaswaste.tsx",
    "src/components/Products/Pay/Payaswaste.tsx",
    "src/components/Products/Earn/Card.jsx"
  ],
  "exclude": ["node_modules"]
}
